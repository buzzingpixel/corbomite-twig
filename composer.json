{
    "name": "buzzingpixel/corbomite-twig",
    "description": "Corbomite Twig",
    "version": "1.2.1",
    "license": "Apache-2.0",
    "homepage": "https://buzzingpixel.com/",
    "require": {
        "php": ">=7.2",
        "buzzingpixel/corbomite-config-collector": "^1.1",
        "buzzingpixel/corbomite-di": "^1.1",
        "buzzingpixel/minify": "^1.1",
        "twig/twig": "^2.7"
    },
    "require-dev": {
        "filp/whoops": "^2.3",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^7.2",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "corbomite\\twig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "corbomite\\tests\\": "tests/"
        }
    },
    "config": {
        "bin-compat": "full",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "diConfigFilePath": "src/diConfig.php",
        "-twigGlobalsFilePath": "path/to/globals.php",
        "-twigExtensions": [
            "some\\vendor\\some\\class"
        ],
        "-twigTemplatesDirectories": {
            "": "path/to/dir",
            "namespace": "path/to/dir"
        }
    }
}
